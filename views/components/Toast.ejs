<%if (isLogedIn) { %>
    <div class="position-absolute top-0 end-0 p-3 mt-5 me-1" style="z-index: 1001">
      <div id="liveToast" class="toast" role="alert" aria-live="assertive" aria-atomic="true"'>
        <div class="toast-header">
          <button type="button" class="btn-close" data-bs-dismiss="toast" aria-label="Close"></button>
        </div>
      <div class="toast-body">
        <p>Are you sure you want to log out?</p>
        <div class='flex-row'>
          <button type="button" class="btn btn-outline-danger btn-toast" data-bs-dismiss="toast" aria-label="Close">Cancel</button>
          <button class='btn btn-outline-success btn-toast' id='logout' >Yes</button>
        </div>
      </div>
    </div>
  </div>
<%} %>

